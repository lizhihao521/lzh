*{margin: 0;padding: 0;font-family: "微软雅黑";
  box-sizing: border-box;list-style: none;}


/******导航栏*************/
/*第一屏容器*/
.header-container{
	width: 100%;
	height: 100%;
}
/*导航栏*/
.nav{
	width: 1200px;
	height: 90px;
	margin: 0 auto;
	font-size: 15px;
}
.left-logo{
	width: 30%;
	height: 100%;
	float: left;
	padding-top: 30px;
	
}
.right-nav{
	width: 70%;
	height: 100%;
	float: right;
}
.right-nav ul{
	width: 80%;
	height: 90px;
	float: left;
}
.right-nav ul li{
	width: 14.2%;
	height: 100%;
	float: left;
	text-align: center;
	line-height: 90px;
}
.right-nav ul li:first-child a{
	color: #ff000e;
	border-bottom: 3px solid #ff000e;
}
.right-nav ul li a{
	text-decoration: none;
	width: 100%;
	height: 100%;
	display: inline-block;
	color: #000000;
}
.right-nav ul li a:hover{
	color: #ff000e;
	border-bottom: 3px solid #ff000e;
}
.right-footer{
	width: 20%;
	height: 90px;
	float: right;
	text-align: right;
	line-height: 90px;
	background: url(../img/导航栏/phone.png)no-repeat;
	background-position:5px;
}
/***第一屏***（*-*）****/
/*******轮播--swiper插件*********************/
.scroll-content{
	width: 100%;
	height: 600px;
}
.swiper-container{
	   height: 100%;
}
.swiper-slide img{
	width: 100%;
	height: 100%;
}
/*--页脚----------------*/
.header-container-footer{
	width:1200px;
	height: 240px;
	margin: 0 auto;
}
.header-container-footer p:first-child{
	text-align: center;
	padding-top: 70px;
    font-size: 34px;
    color: #000;
	
}
.header-container-footer p span{
	    font-size: 40px;
  		color: #ff000e;
}
.header-container-footer p:last-child{
			text-align: center;
	        padding-top: 22px;
    		font-size: 22px;
    		color: #999;
}
/********第二屏*******************/
/*容器*/
.service-container{
	width: 100%;
	height: 100%;
}
.serevice-content{
	    width: 1200px;
    	height: 500px;
   		margin: 0 auto;
    	margin-top: 100px;
}
.col-33{
	width: 400px;
    float: left;
}
.col-33-pic{
	width: 262px;
	height: 302px;
	margin: 0 auto;
}
