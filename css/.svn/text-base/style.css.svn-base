.col-33 p:nth-child(2){
	font-size: 28px;
	color: #333;
	text-align: center;
	margin: 0;
	margin-top: 52px;
}
.col-33 p:last-child{
	font-size: 22px;
	color: #666;
	text-align: center;
	margin: 0;
	margin-top: 30px;
}
/**************预定按钮**************/
.yuding-btn{
	display: block;
	border-radius: 10px;
    width: 190px;
    height: 60px;
    background: #ff000e;
    color: #fff;
    font-size: 20px;
    text-align: center;
    line-height: 60px;
    margin: 70px auto;
    text-decoration: none;
}

.yuding-btn:hover{
    color: #fff;
    text-decoration: none;
}
	
.service-container p{
	display: block;
	text-align: center;
    font-size: 34px;
    color: #000;
    margin-top: 60px;
}
.service-container p span{
    font-size: 40px;
    color: #ff000e;
}
/*********第二屏*********************/
.hideImg{
	display: none;
}

/*  第三屏开始  */
.aside-container{
	width: 100%;
	height: 100%;
}
.index-top{
		width: 1200px;
		height: 30px;
		margin: 80px auto;
		margin-bottom: 0px;
		/*overflow: hidden;*/
}
.col-13{
		width: 220px;
		height: 60px;
		border: 1px solid red;
		border-radius:10px ;
		text-align: center;
		line-height: 60px;
		float: left;
		font-size: 28px;
		margin-right: 40px;
		margin-left: 35px;
		color: red;
	  cursor: pointer;
}
.index-top ul .active{
		color: #FFFFFF;
		background: red;
	}
	
.index-content{
		width: 500px;
		height: 60px;
		margin-top: 10px;
		margin: 80px auto;
		margin-bottom: 0;
		text-align: center;
	}
.a{
		width: 1200px;
		height: 60px;
		margin: 0 auto;
		text-align: center;
}
.a li{
		width: 140px;
		height: 46px;
		border: 1px solid red;
		border-radius: 10px;
		float: left;
		font-size: 22px;
		text-align: center;
		line-height: 46px;
		color: red;
		cursor: pointer;
		margin-right:15px;
}
