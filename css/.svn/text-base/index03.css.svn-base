	width: 60px;
	height: 60px;
	background: url(../img/images/wechatcode.png)no-repeat;
	position: fixed;
	top: 160px;
	right: 100px;
	cursor: pointer
}
.right-bg{
	width: 116px;
	height: 110px;
	background: url(../img/images/codepic.png)no-repeat;
	position: fixed;
	top: 160px;
	right: 170px;
}
.right-mnue2{
	width: 60px;
	height: 60px;
	background: url(../img/images/lineserver.png)no-repeat;
	position: fixed;
	top: 230px;
	right: 100px;
	cursor: pointer
}
.right-mnue3{
	width: 60px;
	height: 60px;
	background: url(../img/images/backtop.png)no-repeat;
	position: fixed;
	top: 300px;
	right: 100px;
	cursor: pointer
}
.right-mnue3:hover{
	background: url(../img/images/gotop.png)no-repeat;
}

