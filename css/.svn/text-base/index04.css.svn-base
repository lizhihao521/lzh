*{margin: 0;padding: 0;font-family: "微软雅黑";
  box-sizing: border-box;list-style: none;}
a{
	text-decoration: none;
}
li{
	list-style: none;
}

/******导航栏*************/
/*第一屏容器*/
.header-container{
	width: 100%;
	height: 100%;
}
/*导航栏*/
.nav{
	width: 1200px;
	height: 90px;
	margin: 0 auto;
	font-size: 15px;
}
.left-logo{
	width: 30%;
	height: 100%;
	float: left;
	padding-top: 30px;
	
}
.right-nav{
	width: 70%;
	height: 100%;
	float: right;
}
.right-nav ul{
	width: 80%;
	height: 90px;
	float: left;
}
.right-nav ul li{
	width: 14.2%;
	height: 100%;
	float: left;
	text-align: center;
	line-height: 90px;
}
.right-nav ul li:nth-child(7) a{
	color: #ff000e;
	border-bottom: 3px solid #ff000e;
}
.right-nav ul li a{
	text-decoration: none;
	width: 100%;
	height: 100%;
	display: inline-block;
	color: #000000;
}
.right-nav ul li a:hover{
	color: #ff000e;
	border-bottom: 3px solid #ff000e;
}
.right-footer{
	width: 20%;
	height: 90px;
	float: right;
	text-align: right;
	line-height: 90px;
	background: url(../img/导航栏/phone.png)no-repeat;
	background-position:5px;
}

/*                *****第一屏图片******              */

.header-pic{
	width: 100%;
	height: 400px;
	margin: 0 auto;
	position: relative;
}
.header-pic img{
	width: 100%;
	height: 400px;
}
.header-pic a{
	width: 190px;
	height: 60px;
	color: #FFFFFF;
	background: red;
	text-align: center;
	line-height: 60px;
	border-radius: 10px;
	display: block;
	font-size: 20px;
	position: absolute;
	top: 75%;
	left: 66%;
}
.header-pic a:hover{
	opacity: 0.8;
}

.header-pic p{
	width: 100%;
	text-align: center;
	font-size: 30px;
	color: #333333;
	padding-top: 50px;
}
.header-pic p>span{
	font-size: 34px;
	color: red;
}

/*                第一屏结束                          */



/*                 第二屏开始                  */

.service-container{
	width: 100%;
	height: 100%;
}
.serevice-content{
	width: 1300px;
	height: 600px;
	margin: 0 auto;
	position: relative;
}
.contener-no1{
	width: 38%;
	float: left;
	margin-top: 150px;
	margin-left: 55px;
}
.contener-no1>p{
	font-size: 25px;
	color: red;
	margin-top: 20px;
	margin-bottom: 40px;
}
.contener-no1 span{
	width: 100%;
	display: block;
	font-size: 20px;
	color: #333333;
}
.contener-no2{
	width: 50%;
	float: left;
	margin-top: 130px;
}
.contener-footer{
	width: 190px;
	height: 60px;
	color: #FFFFFF;
	background: red;
	text-align: center;
	line-height: 60px;
	font-size: 20px;
	border-radius: 10px;
	clear: both;
	cursor: pointer;
	position: absolute;
	left: 5%;
	top: 70%;
}
.contener-footer:hover{
	opacity: 0.8;
}

.serevice-content>p{
	clear: both;
	width: 100%;
	color: #333333;
	text-align: center;
	font-size: 30px;
	padding-top: 60px;
}
.serevice-content p>span{
	font-size: 34px;
	color: red;
}

/*             第二屏结束                */


/*             第三屏开始                 */


.aside-container{
	width: 100%;
	height: 100%;
}
.index-out{
	width: 1300px;
	height: 548px;
	margin: 0 auto;
}
.index-no1{
	width: 30%;
	height: 50%;
	margin-top: 60px;
	margin-left: 70px;
	float: left;
}
.ty{
	width: 85%;
	height: 50%;
	float: left;
	margin-top: 30px;
}
.ty span:first-child{
	font-size: 60px;
	width: 70px;
	height: 85px;
	color: #CCCCCC;
	display: inline-block;
	float: left;
}
.ty span:nth-child(2){
	width: 250px;
	height: 85px;
	display: inline-block;
	color: #666666;
	font-size: 24px;
	line-height: 85px;
	margin-left: 10px;
}
.ty span:last-child{
	width: 250px;
	height: 115px;
	display: inline-block;
	font-size: 16px;
	float: right;
	color: #666666;
}


.index-pic{
	width: 25%;
	height: 60%;
	float: left;
	text-align: center;
	margin-top: 60px;
	
}


.index-no3{
	width: 30%;
	height: 50%;
	float: right;
	margin-right: 30px;
	margin-top: 60px;
}


/*          底部                */


/*          下面随屏滚动部分                     */

.banner-no6{
	width: 100%;
	height: 60px;
	margin: 0 auto;
	background: #4c4c4c;
	/*z-index: 998;*/
}
.banner-conter{
	width: 1200px;
	height: 60px;
	margin: 0 auto;
}
#from1 p{
	display: inline-block;
}
#from1 p:first-child{
	margin-left: 120px;
}
#from1 p > input{
	width: 210px;
	height: 32px;
	border: 1px solid #FFFFFF;
	margin-left: 15px;
	margin-top:14px ;
	font-size: 16px;
}
#from1 #stage{
	width: 90px;
	height: 32px;
	border: none;
	margin-left: 5px;
}
#from1 #grade{
	width: 90px;
	height: 32px;
	border: none;
	margin-left: 5px;
}
#from1 #subject{
	width: 90px;
	height: 32px;
	border: none;
	margin-left: 5px;
}


#from1 .submit{
	width: 125px;
	height: 40px;
	background: red;
	color: #FFFFFF;
	text-align: center;
	line-height: 40px;
	border-radius: 10px;
	margin-left: 20px;
}





/*       footer底部                         */
.footer{
	width: 100%;
	height: 60px;
	background: #333;
	margin: 0 auto;
	text-align: center;
	font-size: 10px;
}
.footer p{
	width: 100%;
	margin: 0 auto;
	line-height: 40px;
	color: #DDDDDD;
}
.footer a{
	color: #DDDDDD;
}
.submit:hover{
	opacity: 0.8;
}
/*     底部结束                            */





