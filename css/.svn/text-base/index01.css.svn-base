}
#from1 p{
	display: inline-block;
}
#from1 p:first-child{
	margin-left: 120px;
}
#from1 p > input{
	width: 210px;
	height: 32px;
	border: 1px solid #FFFFFF;
	margin-left: 15px;
	margin-top:14px ;
	font-size: 16px;
}
#from1 #stage{
	width: 90px;
	height: 32px;
	border: none;
	margin-left: 5px;
}
#from1 #grade{
	width: 90px;
	height: 32px;
	border: none;
	margin-left: 5px;
}
#from1 #subject{
	width: 90px;
	height: 32px;
	border: none;
	margin-left: 5px;
}


#from1 .submit{
	width: 125px;
	height: 40px;
	background: red;
	color: #FFFFFF;
	text-align: center;
	line-height: 40px;
	border-radius: 10px;
	margin-left: 20px;
}





/*       footer底部                         */
.footer{
	width: 100%;
	height: 60px;
	background: #333;
	margin: 0 auto;
	text-align: center;
	font-size: 10px;
}
.footer p{
	width: 100%;
	margin: 0 auto;
	line-height: 40px;
	color: #DDDDDD;
}
.footer a{
