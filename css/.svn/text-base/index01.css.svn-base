*{
	margin: 0;
	padding: 0;
	box-sizing: border-box;
}
li{
	list-style: none;
}
a{
	text-decoration: none;
}

/*        导航栏开始       */
.header{
	width: 100%;
	height: 90px;
	position: fixed;
	background: #FFFFFF;
	top: 0;
	left: 0;
}
.nav{
	width: 1200px;
	height: 90px;
	margin: 0 auto;
}
.left-logo{
	width: 216px;
	height: 30px;
	padding-top: 30px;
	display: inline-block;
}
.right-nav{
	width: 800px;
	height: 90px;
	float: right;
}
.right-nav ul{
	margin: 0;
	padding: 0;
	width: 70%;
	height: 100%;
	float: left;
}
.right-nav ul li{
	width: 80px;
	height: 87px;
	text-align: center;
	float: left;
	line-height: 90px;
}
.right-nav ul li a{
	width: 100%;
	height: 100%;
	display: block;
	color: #000000;
}
.right-nav ul li:nth-child(2) a{
	border-bottom: 3px solid red;
	color: red;
}
.right-nav ul li a:hover{
	border-bottom: 3px solid red;
	color: red;
}
.right-footer{
	width: 27%;
	height: 90px;
	float: right;
	line-height: 90px;
}
.right-footer img{
	width: 19px;
	height: 19px;
}

/*         导航栏结束                        */
.right-mnue1{
	width: 60px;
	height: 60px;
	background: url(../img/images/wechatcode.png)no-repeat;
	position: fixed;
	top: 160px;
	right: 100px;
	cursor: pointer
}
.right-bg{
	width: 116px;
	height: 110px;
	background: url(../img/images/codepic.png)no-repeat;
	position: fixed;
	top: 160px;
	right: 170px;
}
.right-mnue2{
	width: 60px;
	height: 60px;
	background: url(../img/images/lineserver.png)no-repeat;
	position: fixed;
	top: 230px;
	right: 100px;
	cursor: pointer
}
.right-mnue3{
	width: 60px;
	height: 60px;
	background: url(../img/images/backtop.png)no-repeat;
	position: fixed;
	top: 300px;
	right: 100px;
	cursor: pointer
}
.right-mnue3:hover{
	background: url(../img/images/gotop.png)no-repeat;
}



/*         右侧固定栏                           */


/*                 中间部分开始                            */
.banner-no1{
	width: 100%;
	height: 600px;
	background: url(../img/images/flowchart/flowchartbg.png)no-repeat;
	background-size: 100%;
	background-position: center center;
}
.banner-content{
	width: 800px;
	height: 400px;
	margin: 0 auto;
	text-align: center;
	padding-top: 200px;
}
.banner-content p{
	font-size: 34px;
	color: #000000;
}
.banner-content  span{
	color: red;
	font-size: 34px;
	display: block;
	margin-top: 50px;
}
.banner-content .banner-footer{
	width: 190px;
	height: 50px;
	background: red;
	border-radius:10px;
	display: inline-block;
	color:#FFFFFF;
	font-size: 20px;
	line-height: 50px;
	margin-top: 50px;
	cursor: pointer;
}
.banner-content .banner-footer:hover{
	opacity: 0.8;
}


/*          中间一部分结束                                 */

